<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="droppy_aboutus" pattern="/aboutus">
    	<default key="_controller">DroppyMainBundle:StaticPages:aboutus</default>
    </route>
	
	<route id="droppy_privacypolicy" pattern="/privacypolicy">
        <default key="_controller">DroppyMainBundle:StaticPages:privacypolicy</default>
    </route>
	
	<route id="droppy_rule" pattern="/rule">
        <default key="_controller">DroppyMainBundle:StaticPages:rule</default>
    </route>
   
	<route id="droppy_news" pattern="/news">
        <default key="_controller">DroppyMainBundle:StaticPages:news</default>
	</route>

</routes>

